{
  "version": 2,
  "builds": [
    {
      "src": "website/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/website/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/website/$1"
    }
  ],
  "functions": {
    "website/index.html": {
      "runtime": "@vercel/static"
    }
  }
}
